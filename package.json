{
  "name": "testing-size-limit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "size": "npm run build && size-limit",
    "build": "next build",
    "start": "next start",
    "lint": "eslint"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.5",
    "lodash": "^4.17.21",
    "next": "16.0.1",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "size-limit": [
    {
      "limit": "35 kB",
      "path": ".next/*"
    },
    {
      "path": ".next/static/*"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-app": "^11.2.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
